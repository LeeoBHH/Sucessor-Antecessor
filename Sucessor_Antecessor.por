programa {
  funcao inicio() {
    //Declarações de variáveis
    inteiro numero, sucessor, antecessor
     

    //Solicitando dados.

    escreva("Digite o número: ")
    leia(numero)


    //Calculando.
    sucessor = numero + 1
    antecessor = numero - 1



    //Exibindo resultados.
    escreva("\nNumero Selecionado: " + numero)
    escreva("\nSucessor: " + sucessor)
    escreva("\nAntecessor:" + antecessor)


    
  }
}
